{
  "name": "Android SDK Search",
  "description": "Omnibox search of the Android SDK classes",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_popup": "hello.html"
  },
  "background": {
    "scripts": [
      "sdk-search.js"
    ]
  },
  "omnibox": {
    "keyword": "ad2"
  },
  "permissions": [
    "storage",
    "https://developer.android.com/"
  ]
}
