{
  "name": "Android SDK Search",
  "description": "Omnibox search of the Android SDK classes",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "hello.html"
  },
  "background": {
    "scripts": [
      "sdk-search.js"
    ]
  },
  "omnibox": {
    "keyword": "ad2"
  },
  "permissions": [
    "https://developer.android.com/"
  ]
}
